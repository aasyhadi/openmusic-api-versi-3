{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "indent": ["error", 4],
        "camelcase": "off",
        "max-len": 0
    }
}
